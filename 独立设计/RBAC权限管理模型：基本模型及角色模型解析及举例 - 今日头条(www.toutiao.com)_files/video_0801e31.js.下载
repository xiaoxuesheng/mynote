!function(){for(var t,e,r,o,i=document.querySelectorAll(".tt-video-box"),n=0,d=i.length;d>n;n++)e=i[n],t=Math.random().toString(36).substring(2,7).replace(/^\d/,"s"),r=e.getAttribute("tt-videoid"),o=e.getAttribute("tt-poster"),e.setAttribute("id",t),e.setAttribute("class","tt-video-box ttplayer-js"),e.setAttribute("oncontextmenu","return false"),new Player({videoID:r,id:t,width:"640",height:"360",autoplay:!1,poster:o}).start()}(window,document);