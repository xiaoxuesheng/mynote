!function(){for(var t,e,i,r,o=document.querySelectorAll(".tt-video-box"),n=0,u=o.length;u>n;n++)e=o[n],t=Math.random().toString(36).substring(2,7).replace(/^\d/,"s"),i=e.getAttribute("tt-videoid"),r=e.getAttribute("tt-poster"),e.setAttribute("id",t),e.setAttribute("class","tt-video-box ttplayer-js"),e.setAttribute("oncontextmenu","return false"),new Player({videoID:i,id:t,width:"640",height:"360",autoplay:!1,pluginSwitcher:!1}).start()}(window,document);